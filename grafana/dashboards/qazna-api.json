{
  "title": "Qazna API",
  "editable": true,
  "panels": [
    {
      "type": "stat",
      "title": "In-Flight Requests",
      "datasource": {"type": "prometheus", "uid": "Prometheus"},
      "targets": [{"expr": "http_inflight_requests"}],
      "gridPos": {"x": 0, "y": 0, "w": 6, "h": 6}
    },
    {
      "type": "graph",
      "title": "HTTP Requests Rate",
      "datasource": {"type": "prometheus", "uid": "Prometheus"},
      "targets": [{"expr": "rate(http_requests_total[1m])"}],
      "gridPos": {"x": 6, "y": 0, "w": 18, "h": 6}
    },
    {
      "type": "heatmap",
      "title": "Latency (histogram)",
      "datasource": {"type": "prometheus", "uid": "Prometheus"},
      "targets": [{"expr": "sum(rate(http_request_duration_seconds_bucket[1m])) by (le)"}],
      "gridPos": {"x": 0, "y": 6, "w": 24, "h": 9}
    }
  ],
  "schemaVersion": 36,
  "version": 1,
  "timezone": "browser"
}
